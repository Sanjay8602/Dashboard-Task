{
    "manifest_version": 3,
    "name": "LinkedIn AI Reply Assistant",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/*"],
        "js": ["popup.js"],
        "css": ["index.css"]
      }
    ]
  }
  